{
  "name": "csrf-demo",
  "version": "1.0.0",
  "description": "simple demonstration of csrf vulnerability and mitigation",
  "main": "index.js",
  "scripts": {
    "start": "npx webpack -p && env NODE_ENV=production node server/index.js",
    "start-dev": "npx webpack -d && env NODE_ENV=development node server/index.js",
    "seed": "node server/scripts/seed.js",
    "start-evil": "npx http-server evil-site",
    "build": "npx webpack -p",
    "start-server": "env NODE_ENV=production node server/index.js",
    "start-win": "npx webpack -p && set NODE_ENV=production && node server\\index.js",
    "start-dev-win": "npx webpack -d && set NODE_ENV=development && node server\\index.js",
    "seed-win": "node server\\scripts\\seed.js"
  },
  "author": "Brook Li",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "bcrypt": "^3.0.6",
    "cookie-parse": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.9",
    "csurf": "^1.10.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "http-server": "^0.11.1",
    "memorystore": "^1.6.1",
    "path": "^0.12.7",
    "pg-promise": "^8.6.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.12"
  }
}
